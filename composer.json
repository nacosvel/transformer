{
    "name": "nacosvel/transformer",
    "description": "rule-driven data projection and transformation.",
    "keywords": [
        "Nacosvel",
        "nacosvel",
        "Transformer",
        "transformer"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/nacosvel/transformer",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "nacosvel/pipeline": "^1.1"
    },
    "require-dev": {
        "code-lts/doctum": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Nacosvel\\Transformer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "docs": "@php vendor/bin/doctum.php update doctum.php"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
